{
  "expo": {
    "name": "safeBus",
    "slug": "safeBus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/safeBuslogo1.png",
    "scheme": "safebus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDZthrLZxOUbCW7pJNFi-AtGN-qTIwJnNU"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to help you pick start and end points on the map."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/safeBuslogo1.png",
        "backgroundColor": "#1F62FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rumeshsmrr.safeBus",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDZthrLZxOUbCW7pJNFi-AtGN-qTIwJnNU"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/safeBuslogo1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/safeBuslogo1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1F62FF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY": "AIzaSyDZthrLZxOUbCW7pJNFi-AtGN-qTIwJnNU"
    }
  }
}
